<Window x:Class="Kino_Umudillo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kino_Umudillo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="frame"  NavigationUIVisibility="Hidden" Margin="0,40,0,0"/>
        <Button Click="Clubs" Content="Клубы" Height="25" Margin="10,10,0,0" 
         Background="#FF6F109C" Foreground="White" BorderBrush="#FF32BEA6" 
         VerticalAlignment="Top" HorizontalAlignment="Left" Width="100" />
        <Button Click="Users" Content="Пользователи" Height="25" Margin="115,10,0,0"
         Background="#FF6F109C" Foreground="White" BorderBrush="#FF32BEA6" 
         VerticalAlignment="Top" HorizontalAlignment="Left" Width="100" />
    </Grid>
</Window>
